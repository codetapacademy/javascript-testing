{
  "name": "javascript-testing",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:w": "jest --watch",
    "flow": "flow",
    "babel-node": "babel-node --presets=@babel/env",
    "start": "nodemon --exec npm run babel-node -- ./index.js",
    "prettier": "prettier \"**/*.+(js|jsx|json|css|less|sass|yml|yaml|ts|tsx|md|mdx|graphql|vue)\"",
    "lint": "eslint src",
    "lints": "lint-staged",
    "validate": "npm run lint && npm run test && npm run prettier -- --list-different && npm run flow",
    "format": "npm run prettier -- --write",
    "precommit": "lint-staged && npm run flow"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "eslint": "^5.12.0",
    "eslint-config-prettier": "^3.4.0",
    "flow-bin": "^0.90.0",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{js,scss}": [
        "npm run prettier",
        "git add"
      ]
    },
    "ignore": [
      "**/dist/*.min.js"
    ],
    "env": {
      "jest": true
    }
  }
}
